	<content><![CDATA[
int kmp(string const& s) {
    int n = s.size();
    vector<int> lps(n, 0);
    for (int i = 1; i < n; i++) {
        int j = lps[i - 1];
        while (j > 0 && s[i] != s[j])
            j = lps[j - 1];
        if (s[i] == s[j]) j++;
        lps[i] = j;
    }
    return lps.back();
}
void kmp_aut(string s, vector<vector<int>>& aut) {
    s += '#';
    char base = 'a';
    int n = s.size();
    vector<int> pi = kmp(s);
    aut.assign(n, vector<int>(26));
    for (int i = 0; i < n; i++) {
        for (int c = 0; c < 26; c++) {
            if (i > 0 && base + c != s[i])
                aut[i][c] = aut[pi[i - 1]][c];
            else
                aut[i][c] = i + (base + c == s[i]);
        }
    }
}
]]></content>
